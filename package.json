{
	"name": "obsidian-family-tree",
	"version": "1.0.0",
	"description": "Family Tree plugin for Obsidian - visualize and manage family relationships",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint ."
	},
	"keywords": ["obsidian", "family-tree", "genealogy", "visualization"],
	"license": "0-BSD",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@types/cytoscape": "^3.21.0",
		"esbuild": "0.25.5",
		"eslint-plugin-obsidianmd": "0.1.9",
		"globals": "14.0.0",
		"tslib": "2.4.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "8.35.1",
		"@eslint/js": "9.30.1",
		"jiti": "2.6.1"
	},
	"dependencies": {
		"obsidian": "latest",
		"cytoscape": "^3.28.1",
		"cytoscape-dagre": "^2.5.0",
		"cytoscape-fcose": "^2.2.0",
		"cytoscape-edgehandles": "^4.0.1"
	}
}
